{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "capabilities": [
        {
          "identifier": "main-capability",
          "permissions": [
            "core:window:allow-start-dragging",
            "core:default",
            "dialog:allow-message",
            "dialog:allow-ask",
            "dialog:allow-confirm",
            "dialog:default",
            {
              "allow": [
                {
                  "url": "https://github.com/**"
                }
              ],
              "identifier": "opener:allow-open-url"
            }
          ],
          "windows": [
            "main"
          ]
        }
      ],
      "csp": null
    },
    "windows": [
      {
        "decorations": true,
        "height": 800,
        "title": "Git管理器",
        "transparent": false,
        "width": 1200
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "dmg": {
        "appPosition": {
          "x": 240,
          "y": 300
        },
        "applicationFolderPosition": {
          "x": 790,
          "y": 300
        },
        "background": "assets/dmg-background.jpg",
        "windowSize": {
          "height": 678,
          "width": 1025
        }
      }
    },
    "resources": [
      "resources/fix-macos.command",
      "resources/README-macOS.txt"
    ],
    "targets": "all"
  },
  "identifier": "com.caogit.app",
  "productName": "CaoGit",
  "version": "0.2.19"
}